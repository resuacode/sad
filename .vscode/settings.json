{
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "mdx": true
  },
  "files.associations": {
    "*.mdx": "mdx"
  },
  "[mdx]": {
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "editor.tabCompletion": "on"
  }
}
